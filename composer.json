{
    "name": "jpmurray/doddns",
    "description": "Small tool to use Digital Ocean as a Dynamic DNS (DDNS) service.",
    "keywords": ["digitalocean", "digital ocean", "ddns", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/jpmurray/doddns",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jpmurray/doddns/issues",
        "source": "https://github.com/jpmurray/doddnslaravel-zero"
    },
    "authors": [
        {
            "name": "Jean-Phlippe Murray",
            "email": "jp@atomescroch.us"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.3",
        "laravel-zero/framework": "5.8.*",
        "nunomaduro/laravel-console-menu": "^2.1",
        "toin0u/digitalocean-v2": "~2.0",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["./builds/doddns"]
}
